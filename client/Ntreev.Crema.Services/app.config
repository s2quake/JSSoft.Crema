<configuration>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_IDataBaseService">
                    <security mode="None" />
                </binding>
                <binding name="NetTcpBinding_IDescriptorService">
                    <security mode="None" />
                </binding>
                <binding name="NetTcpBinding_IDataBaseContextService">
                    <security mode="None" />
                </binding>
                <binding name="NetTcpBinding_IDomainContextService">
                    <security mode="None" />
                </binding>
                <binding name="NetTcpBinding_IUserContextService">
                    <security mode="None" />
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:4004/DataBaseService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IDataBaseService"
                contract="DataBaseService.IDataBaseService" name="NetTcpBinding_IDataBaseService" />
            <endpoint address="net.tcp://localhost:4004/DescriptorService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IDescriptorService"
                contract="DescriptorService.IDescriptorService" name="NetTcpBinding_IDescriptorService" />
            <endpoint address="net.tcp://localhost:4004/DataBaseContextService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IDataBaseContextService"
                contract="DataBaseContextService.IDataBaseContextService"
                name="NetTcpBinding_IDataBaseContextService" />
            <endpoint address="net.tcp://localhost:4004/DomainContextService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IDomainContextService"
                contract="DomainContextService.IDomainContextService" name="NetTcpBinding_IDomainContextService" />
            <endpoint address="net.tcp://localhost:4004/UserContextService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IUserContextService"
                contract="UserContextService.IUserContextService" name="NetTcpBinding_IUserContextService" />
        </client>
    </system.serviceModel>
</configuration>